{
  "name": "qiscus-sdk-javascript",
  "version": "3.2.0",
  "description": "Qiscus Web SDK Core",
  "license": "MIT",
  "main": "dist/qiscus-sdk-js.cjs.js",
  "module": "dist/qiscus-sdk-js.esm.js",
  "browser": "dist/qiscus-sdk-javascript.umd.js",
  "typings": "types/index.d.ts",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "lib",
    "types",
    "lib-esm"
  ],
  "scripts": {
    "serve": "serve dist -l 1235",
    "clean": "rimraf dist lib lib-esm types",
    "build": "run-s clean build:all esbuild",
    "build:all": "run-p build:bundle build:bundle:dev build:lib:cjs build:lib:esm",
    "build:lib:cjs": "tsc -m commonjs --outDir lib",
    "build:lib:esm": "tsc -m es2015 --outDir lib-esm",
    "build:bundle": "webpack --env production --env target=web",
    "build:bundle:watch": "webpack --env development --env target=web --watch",
    "build:bundle:dev": "webpack --env development --env target=web",
    "test": "jest",
    "esbuild": "run-p esbuild:cjs esbuild:esm",
    "esbuild:cjs": "esbuild src/index.ts --platform=node --bundle --format=cjs --outfile=dist/qiscus-sdk-js.cjs.js",
    "esbuild:esm": "esbuild src/index.ts --platform=node --bundle --format=esm --outfile=dist/qiscus-sdk-js.esm.js",
    "esbuild:cjs:min": "npm run esbuild:cjs -- --minify",
    "esbuild:esm:min": "npm run esbuild:esm -- --minify"
  },
  "prettier": {
    "semi": false,
    "useTabs": false,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@babel/runtime": "^7.14.6",
    "@sindresorhus/is": "^4.0.1",
    "@toolbuilder/await-for-it": "^1.1.0",
    "axios": "^0.21.1",
    "buffer": "^6.0.3",
    "core-js": "^3.15.2",
    "derivable": "^2.0.0-beta.3",
    "is_js": "^0.9.0",
    "kefir": "^3.8.8",
    "lodash.debounce": "^4.0.8",
    "lodash.flatten": "^4.4.0",
    "mqtt": "^4.2.8",
    "pietile-eventemitter": "^1.0.1",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@types/is": "0.0.21",
    "@types/jest": "^26.0.23",
    "@types/kefir": "^3.8.7",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.flatten": "^4.4.6",
    "@types/node": "^15.12.4",
    "@types/ws": "^7.4.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-macros": "^3.1.0",
    "esbuild": "^0.12.9",
    "fork-ts-checker-webpack-plugin": "^6.2.12",
    "microbundle": "^0.13.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "serve": "^12.0.0",
    "ts-loader": "^8.3.0",
    "typescript": "^4.3.4",
    "webpack": "4",
    "webpack-cli": "^4.7.2"
  }
}
